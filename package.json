{
  "name": "bitcoin-barber-website",
  "version": "1.2.1",
  "description": "Bitcoin Barber",
  "author": "Richard Ayotte",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production nuxt build --universal",
    "dev": "cross-env NODE_ENV=development nodemon --harmony server/index.js --watch server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn run lint",
    "ship-production": "shipit production deploy",
    "start": "cross-env NODE_ENV=production node --harmony server/index.js",
    "test": "yarn --silent intern; true"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "config": "^3.3.2",
    "cross-env": "^7.0.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.20",
    "nodemailer": "^6.4.16",
    "nuxt": "^2.14.7",
    "nuxt-start": "^2.14.7",
    "rucksack-css": "^1.0.2"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "cache-loader": "^4.1.0",
    "eslint": "^7.13.0",
    "eslint-config-ayotte": "^1.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.1.0",
    "intern": "^4.9.0",
    "nodemon": "^2.0.6",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "terser-webpack-plugin": "^5.0.3",
    "thread-loader": "^3.0.1"
  },
  "repository": "https://github.com/RichAyotte/bitcoin-barber-website.git",
